

#include <stdio.h>

int main() {
    // Declaring and initializing a 2D array named 'scores' with 2 rows and 2 columns
    int scores[2][2] = {
        {85, 90},    // First row values
        {78, 88}     // Second row values
    };

    // Printing the elements of the 2D array using a nested for loop
    printf("Scores array elements:\n");
    for (int i = 0; i < 2; i++) { // Outer loop for rows
        for (int j = 0; j < 2; j++) { // Inner loop for columns
            printf("scores[%d][%d] = %d\n", i, j, scores[i][j]);
        }
    }

    return 0;
}
